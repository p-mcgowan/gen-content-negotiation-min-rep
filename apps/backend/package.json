    "recursive-readdir-sync": "^1.0.6",
    "request-ip": "^3.3.0",
    "tslib": "^2.6.3",
    "winston": "^3.13.1",
    "worker-farm": "^1.7.0",
    "yamljs": "^0.3.0"
  },
