  "scripts": {
    "build": "tsc && tsc-alias",
    "watch": "tsc-watch --onSuccess 'bash -c \"npx tsc-alias && echo done\"'",
    "test": "node -r source-map-support/register --test 'dist/test/**/*.spec.js'",
    "g": "node -r ./no-update.js node_modules/.bin/generate-it --dont-run-comparison-tool -yt $GEN_TPL ../../spec/release/$GEN_SPEC.json",
    "postg": "bash postgen.sh",
    "gen": "GEN_TPL=~/source/acrontum/open-source/generate-it-typescript-server GEN_SPEC=backend npm run g",
    "gen-feat-oas3": "GEN_TPL=https://github.com/acrontum/generate-it-typescript-server#fix/content-negotiation GEN_SPEC=backend npm run g",
    "gen-feat-oas2": "GEN_TPL=https://github.com/acrontum/generate-it-typescript-server#fix/content-negotiation GEN_SPEC=backend-oa2 npm run g",
    "gen-main-oas3": "GEN_TPL=https://github.com/acrontum/generate-it-typescript-server GEN_SPEC=backend npm run g",
    "gen-main-oas2": "GEN_TPL=https://github.com/acrontum/generate-it-typescript-server GEN_SPEC=backend-oa2 npm run g",
    "lint": "prettier --write test src/domains"
  },
  "dependencies": {
    "celebrate": "^15.0.3",
    "command-line-args": "^5.2.0",
    "cors": "^2.8.5",
